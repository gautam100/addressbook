pipeline{
	agent any
	
	stages{
		stage('compile'){
			steps{
				script{
					echo "COMPILING THE CODE"
				}
			}
		}
		stage('UnitTest'){
			steps{
				script{
					echo "Run the Unit Test case. thank you."
				}
			}
		}
		stage('Package'){
			steps{
				script{
					echo "Package the code"
				}
			}
		}
	}
}
